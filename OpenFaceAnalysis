```{r include=FALSE, echo=FALSE, results='hide'}
mod <- lme(cortvalue ~ 
              timebeforepeak +  #Fixed effect
              timeafterpeak +  #Fixed effect
              baseline +
              AU01_r*timebeforepeak + AU01_r*timeafterpeak + AU01_r*ThreatCompc +   
              AU02_r*timebeforepeak + AU02_r*timeafterpeak + AU02_r*ThreatCompc + 
              AU04_r*timebeforepeak + AU04_r*timeafterpeak + AU04_r*ThreatCompc +  
              AU05_r*timebeforepeak + AU05_r*timeafterpeak + AU05_r*ThreatCompc +
              AU06_r*timebeforepeak + AU06_r*timeafterpeak + AU06_r*ThreatCompc + 
              AU07_r*timebeforepeak + AU07_r*timeafterpeak + AU07_r*ThreatCompc +   
              AU09_r*timebeforepeak + AU09_r*timeafterpeak + AU09_r*ThreatCompc +
              AU10_r*timebeforepeak + AU10_r*timeafterpeak + AU10_r*ThreatCompc +   
              AU12_r*timebeforepeak + AU12_r*timeafterpeak + AU12_r*ThreatCompc + 
              AU14_r*timebeforepeak + AU14_r*timeafterpeak + AU14_r*ThreatCompc +
              AU15_r*timebeforepeak + AU15_r*timeafterpeak + AU15_r*ThreatCompc +
              AU17_r*timebeforepeak + AU17_r*timeafterpeak + AU17_r*ThreatCompc +
              AU20_r*timebeforepeak + AU20_r*timeafterpeak + AU20_r*ThreatCompc +
              AU23_r*timebeforepeak + AU23_r*timeafterpeak + AU23_r*ThreatCompc +
              AU25_r*timebeforepeak + AU25_r*timeafterpeak + AU25_r*ThreatCompc +
              AU26_r*timebeforepeak + AU26_r*timeafterpeak + AU26_r*ThreatCompc,
              random = ~1|id, # Random slopes but not intercepts
              corr = corAR1(), # specifies CAR1 structure
              #control =  ctrl, # optimizes model
              method = "REML", # specifies REML fit
              data = cp_long)
